      <mdException name="net.geoprism.data.etl.excel.ExtraSpreadsheetTabException" label="Spreadsheet Has Extra Tabs Exception" description="Thrown when a user tries to upload a spreadsheet that has more tabs than expected." message="The uploaded spreadsheet has one or more tabs than expected. Remove [{tabLabel}].">
        <attributes>
          <text name="tabLabel" label="Tab Label" />
        </attributes>
      </mdException>
      
      <mdException name="net.geoprism.data.etl.excel.ExtraSpreadsheetTabColumnException" label="Spreadsheet Tab Has Extra Columns Exception" description="Thrown when a user tries to upload a spreadsheet that has a tab with more columns than expected." message="The uploaded spreadsheet has a tab [{tabLabel}] with one or more columns than expected. Remove [{columnLabel}].">
        <attributes>
          <text name="tabLabel" label="Tab Label" />
          <text name="columnLabel" label="Column Label" />
        </attributes>
      </mdException>
      
      <mdException name="net.geoprism.data.etl.excel.MissingSpreadsheetTabColumnException" label="Spreadsheet Tab Missing Columns Exception" description="Thrown when a user tries to upload a spreadsheet that has a tab that does not have all the expected columns." message="The uploaded spreadsheet tab [{tabLabel}] is missing one or more required columns. Add [{columnLabel}].">
        <attributes>
          <text name="tabLabel" label="Tab Label" />
          <text name="columnLabel" label="Column Label" />
        </attributes>
      </mdException>
    
    
