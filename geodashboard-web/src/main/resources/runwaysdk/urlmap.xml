<?xml version="1.0"?>
<mappings>
  <map uri="" forward="/index.jsp"/>
  
  <map uri="DashboardViewer" controller="com.runwaysdk.geodashboard.gis.persist.DashboardMapController">
    <action method="createMapForSession" uri=""/>
  </map>

  <map uri="login" forward="/login.jsp"/>
  <map uri="loginRedirect" forward="/loginRedirect.jsp"/>
  <map uri="session" controller="com.runwaysdk.geodashboard.SessionController">
    <action method=".*" uri="${method}"/>
  </map>
  
  <map uri="q4sales" forward="/com/runwaysdk/geodashboard/jsp/q4sales.jsp"/>
  <map uri="q4sales/inner" forward="/com/runwaysdk/geodashboard/jsp/inner.jsp"/>
  
  <map uri="admin" controller="com.runwaysdk.geodashboard.AdminController">
    <action method=".*" uri="${method}"/>    
  </map>
  <map uri="report" controller="com.runwaysdk.geodashboard.report.ReportItemController">
    <action method=".*" uri="${method}"/>    
  </map>
</mappings>
