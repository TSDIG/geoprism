<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<doIt>
		<create>



			<mdBusiness name="com.runwaysdk.geodashboard.Dashboard"
				label="Dashboard" cacheAlgorithm="everything">
				<attributes>
					<localChar name="displayLabel" label="Name" required="true" />
				</attributes>
				<mdMethod name="getSortedDashboards" label="Get Sorted Dashboards"
					static="true" returnType="com.runwaysdk.geodashboard.DashboardQuery">
				</mdMethod>
				<mdMethod name="getSortedTypes" label="Get Sorted Types"
					description="Returns a sorted list of de-referenced MdClasses stored by MetadataWrapper"
					returnType="com.runwaysdk.system.metadata.MdClass[]">
				</mdMethod>
			</mdBusiness>

			<mdRelationship name="com.runwaysdk.geodashboard.DashboardMetadata" label="Dashboard Metadata"
				cacheAlgorithm="everything">
				<attributes>
				  <integer name="listOrder" required="true"/>
				</attributes>
				<parent cardinality="*" name="com.runwaysdk.geodashboard.Dashboard"
					method="Dashboard" />
				<child cardinality="*"
					name="com.runwaysdk.geodashboard.MetadataWrapper" method="Metadata" />
			</mdRelationship>

			<mdBusiness name="com.runwaysdk.geodashboard.MetadataWrapper"
				label="Dashboard Wrapper" cacheAlgorithm="everything">
				<attributes>
					<reference name="wrappedMdClass" type="com.runwaysdk.system.metadata.MdClass"
						label="MdClass" required="true" />
				</attributes>
				<mdMethod name="getSortedAttributes" label="Get Sorted Attributes"
          description="Returns a sorted list of de-referenced MdAttributes stored by MetadataAttribute"
          returnType="com.runwaysdk.geodashboard.MdAttributeView[]">
        </mdMethod>
			</mdBusiness>
			
			<mdView name="com.runwaysdk.geodashboard.MdAttributeView" label="MdAttribute View">
			  <attributes>
			    <char name="mdClassId" required="true" label="MdClass " size="64"/>
			    <char name="mdAttributeId" required="true" label="MdAttribute " size="64"/>
			    <char name="displayLabel" required="true" label="Display Label" size="255"/>
			  </attributes>
			</mdView>
			
			<mdRelationship name="com.runwaysdk.geodashboard.DashboardAttributes" label="Dashboard Attributes"
				cacheAlgorithm="everything">
				<attributes>
          <integer name="listOrder" required="true"/>
        </attributes>
				<parent cardinality="*" name="com.runwaysdk.geodashboard.MetadataWrapper"
					method="DashboardMetadata" />
				<child cardinality="*"
					name="com.runwaysdk.geodashboard.AttributeWrapper" method="AttributeWrapper" />
			</mdRelationship>
			
			
      <mdBusiness name="com.runwaysdk.geodashboard.AttributeWrapper"
        label="Attribute Wrapper" cacheAlgorithm="everything">
        <attributes>
          <reference name="wrappedMdAttribute" type="com.runwaysdk.system.metadata.MdAttribute"
            label="MdAttribute" required="true" />
        </attributes>
      </mdBusiness>			
			
		</create>
		<update></update>
	</doIt>
	<undoIt>
		<delete>

			<object key="com.runwaysdk.geodashboard.AttributeWrapper" type="com.runwaysdk.system.metadata.MdBusiness" />
			<object key="com.runwaysdk.geodashboard.MetadataWrapper" type="com.runwaysdk.system.metadata.MdBusiness" />
			<object key="com.runwaysdk.geodashboard.DashboardAttributes"
				type="com.runwaysdk.system.metadata.MdRelationship" />
			<object key="com.runwaysdk.geodashboard.DashboardMetadata"
				type="com.runwaysdk.system.metadata.MdRelationship" />
			<object key="com.runwaysdk.geodashboard.Dashboard" type="com.runwaysdk.system.metadata.MdBusiness" />
			<object key="com.runwaysdk.geodashboard.MdAttributeView" type="com.runwaysdk.system.metadata.MdView" />

		</delete>
	</undoIt>
</version>